package assign2_2;

import java.util.Scanner;

public class assign2_2 {

	public assign2_2() {
		// TODO Auto-generated constructor stub
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner scn = new Scanner(System.in);
		int n;
		n=scn.nextInt();
		int a=1;
		while(n!=0){
			a*=n;
			n--;
		}
	}

}
